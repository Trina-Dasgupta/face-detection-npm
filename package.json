{
  "name": "face-scanner-js",
  "version": "1.0.0",
  "description": "Advanced face scanner with facial landmark detection, glasses detection, and comprehensive face quality analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "face-scanner-setup": "./scripts/setup.js"
  },
  "scripts": {
  "build": "webpack --mode=production",
    "dev": "webpack --mode=development --watch",
    "serve": "http-server -p 8000 -c-1",
  "python-setup": "node scripts/setup.js",
    "test": "jest",
    "build:python": "node scripts/build-python.js",
    "build:all": "npm run build:python && npm run build"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.10.0",
    "@tensorflow-models/face-landmarks-detection": "^1.0.1",
    "python-shell": "^5.0.0",
    "bindings": "^1.5.0",
    "node-addon-api": "^7.0.0",
    "node-gyp": "^9.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@types/node": "^20.8.0",
    "typescript": "^5.2.2",
    "babel-loader": "^9.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "face-scanner": {
    "pythonVersion": ">=3.8.0",
    "pythonDependencies": [
      "opencv-python>=4.5.5",
      "numpy>=1.19.5",
      "mediapipe>=0.8.9.1"
    ]
  }
}